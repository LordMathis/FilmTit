<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="cz.filmtit.userspace">

    <class name="USession" table="Sessions">
        <id name="databaseId" type="long" column="Id">
            <generator class="native"/>
        </id>

        <property name="Session" type="java.lang.String" column="session" />
        <property name="SessionFrom" type="java.util.Date" column="from" />
        <property name="SessionTo" type="java.util.Date" column="to" />
        <property name="SessionDoc" type="java.lang.String" column="documents" />
        <property name="SessionDocSize" type="java.lang.String" column="docSize" />
        <property name="SessionFinishedChunks" type="java.lang.String" column="finishedChunks" />
    </class>

</hibernate-mapping>
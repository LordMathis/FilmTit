<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cz.filmtit.userspace.USDocumentUser" table="DocumentUsers">
        <id column="id" name="id">
            <generator class="increment"/>
        </id>
        <property column="userId" name="userId" type="long"/>
        <property column="moviePath" name="moviePath" type="string"/>
        <property column="posteditOn" name="posteditOn" type="boolean"/>
        <property column="localFile" name="localFile" type="boolean"/>
        <property column="autoplay" name="autoplay" type="boolean"/>
    </class>
</hibernate-mapping>

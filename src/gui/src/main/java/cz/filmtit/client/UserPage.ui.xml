<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:b="urn:import:com.github.gwtbootstrap.client.ui">


	<g:HTMLPanel styleName="user_page">
	
		<b:Heading size="3">Your documents</b:Heading>

		<b:TabPanel ui:field="tabPanel" tabPosition="above">
			<b:Tab ui:field="tabDocumentList" heading="List of your documents" active="true" />
			<b:Tab ui:field="tabNewDocument" heading="Create a new document" />
		</b:TabPanel>
        
	</g:HTMLPanel>
	
</ui:UiBinder> 